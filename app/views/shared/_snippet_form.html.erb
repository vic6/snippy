<%= form_for(@snippet) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control' %>
  <%= f.label 'Content' %>
  <%= f.text_area :content, class: 'form-control' %>
  <%= f.submit 'Create snippet', class: 'btn btn-primary' %>
<% end %>
